generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model tabletest {
  ID       Int    @id @default(autoincrement())
  Pesel    String @db.Char(30)
  Email    String @db.Char(30)
  Imie     String @db.Char(30)
  Nazwisko String @db.Char(30)
  Haslo    String @db.Char(30)
}
